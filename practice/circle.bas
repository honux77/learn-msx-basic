  10 SCREEN 2:COLOR 1,15,4
  20 W=256/5
  30 H=192/3
  40 X=W/2
  50 Y=H/2
  60 S=W*.3
  70 C=0
  100 FOR Y2 = 0 TO 2  
  110 FOR X2 = 0 TO 4
